{
  "name": "insider/champions-league",
  "description": "Insider Champions League Simulation",
  "type": "project",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.4",
    "ext-pdo": "*",
    "beberlei/doctrineextensions": "^1.3",
    "doctrine/dbal": "^3.8",
    "doctrine/doctrine-bundle": "^2.14",
    "doctrine/migrations": "^3.7",
    "doctrine/orm": "^2.19",
    "php-di/php-di": "^7.0",
    "slim/csrf": "^1.3",
    "slim/psr7": "^1.6",
    "slim/slim": "^4.12",
    "symfony/cache": "^7.2",
    "symfony/console": "^7.0",
    "symfony/rate-limiter": "^7.0",
    "symfony/webpack-encore-bundle": "^2.1",
    "vlucas/phpdotenv": "^5.6",
    "vlucas/valitron": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "symfony/var-dumper": "^7.0"
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public/",
    "test": "phpunit"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  }
}
